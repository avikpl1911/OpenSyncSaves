<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.AnchorPane?>
<VBox  spacing="10.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.app.main.HelloController">
    <AnchorPane>
        <Button text="Add new" textAlignment="CENTER" AnchorPane.rightAnchor="15" AnchorPane.topAnchor="20" onAction="#onAddNew"/>
    </AnchorPane>


    <AnchorPane
    stylesheets="@style.css"
    >

            <TableView   AnchorPane.leftAnchor="15" AnchorPane.rightAnchor="15" AnchorPane.bottomAnchor="15"
            AnchorPane.topAnchor="50"
             fx:id="tabview"
            >
                <columns>
                    <TableColumn text="Column 1" prefWidth="100" fx:id="col1"/>
                    <TableColumn text="Column 2" prefWidth="200" fx:id="col2"/>
                    <TableColumn text="Column 3" prefWidth="700" fx:id="col3"/>
                </columns>

            </TableView>

    </AnchorPane>



</VBox>
